import cupy
from cupyx.scipy.sparse import csr_matrix
from cupyx.scipy.sparse.csgraph import connected_components
from cupy_backends.cuda.libs import cugraph
data = cupy.array([1, 1, 1, 1], dtype="float32")
indices = cupy.array([1, 2, 2, 4], dtype="float32")
indptr = cupy.array([0, 2, 3, 3, 4, 4], dtype="float32")
graph = csr_matrix((data, indices, indptr))
n_components, labels = connected_components(csgraph=graph, directed=False, return_labels=True)
